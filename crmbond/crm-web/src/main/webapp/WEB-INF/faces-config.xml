<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

   
   <managed-bean>
  <managed-bean-name>utils</managed-bean-name>
  <managed-bean-class>com.crm.util.UtilityList</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
  <managed-property>
   <property-name>interactionstatus</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>PENDING</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>FINISHED</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>WAIT</value>
    </map-entry>
    <map-entry>
     <key>3</key>
     <value>SUSPENDED</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>place</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>ONLINE</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>INTO_OFFICE</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>OUT_OFFICE</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>clientstatusmap</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>LEGAL_STATUS_SHA</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>LEGAL_STATUS_SHPK</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>LEGAL_STATUS_PF</value>
    </map-entry>
    <map-entry>
     <key>3</key>
     <value>STATUS_MATURED</value>
    </map-entry>
    <map-entry>
     <key>4</key>
     <value>STATUS_BEGGINER</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>potentialclient</property-name>
   <map-entries>
    <map-entry>
     <key>2</key>
     <value>STATUS_MATURED</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>STATUS_BEGGINER</value>
    </map-entry>
    <map-entry>
     <key>0</key>
     <value>STATUS_UNKNOWN</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>potentialcontact</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>STATUS_NEW</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>STATUS_SURE</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>STATUS_NOTSURE</value>
    </map-entry>
    <map-entry>
     <key>3</key>
     <value>STATUS_MATURED</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>contractstatus</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>CONTRACT_STATUS_ACTIVE</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>CONTRACT_STATUS_EXPIRED</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>CONTRACT_STATUS_CANCELED</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>requeststatus</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>REQUEST_STATUS_UNDERPROCES</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>REQUEST_STATUS_COMPLETED</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>REQUEST_STATUS_FINISHED</value>
    </map-entry>
    <map-entry>
     <key>3</key>
     <value>REQUEST_STATUS_REVIEW</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>eventstatus</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>TELEPHONE</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>EMAIL</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>MOBILE</value>
    </map-entry>
    <map-entry>
     <key>3</key>
     <value>VISIT_OFFICE</value>
    </map-entry>
    <map-entry>
     <key>4</key>
     <value>OUT_OFFICE</value>
    </map-entry>
   </map-entries>
  </managed-property>
  <managed-property>
   <property-name>interactionservicestatus</property-name>
   <map-entries>
    <map-entry>
     <key>0</key>
     <value>STARTED</value>
    </map-entry>
    <map-entry>
     <key>1</key>
     <value>PENDING</value>
    </map-entry>
    <map-entry>
     <key>2</key>
     <value>FINISHED</value>
    </map-entry>
    <map-entry>
     <key>4</key>
     <value>WAIT</value>
    </map-entry>
    <map-entry>
     <key>5</key>
     <value>SUSPENDED</value>
    </map-entry>
   </map-entries>
  </managed-property>
 </managed-bean>

 <navigation-rule>
  <from-view-id>/*</from-view-id>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/home/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/home/index.xhtml</to-view-id>
   <redirect />
  </navigation-case>
 </navigation-rule>

 
 <application>
  <resource-bundle>
   <base-name>Bundle</base-name>
   <var>bundle</var>
  </resource-bundle>
  <locale-config>
   <default-locale>en</default-locale>
   <supported-locale>sq</supported-locale>
  </locale-config>
 </application>
</faces-config>